    "def factorielle(n):\n",
    "   if n == 0:\n",
    "      return 1\n",
    "   else:\n",
    "      J = 1\n",
    "      for k in range(2,n+1):\n",
    "         J = J * k\n",
    "      return J\n",
    "factorielle(5)"
   ]
